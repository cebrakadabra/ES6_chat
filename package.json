{
  "name": "ES6_chat",
  "version": "1.0.0",
  "main": "Gruntfile.js",
  "private": true,
  "description": "a chat application that uses ES6 in frontend and nodejs with mongoDB as backend",
  "scripts": {
    "postinstall": "napa",
    "test": "npm test",
    "start": "node app"
  },
  "napa": {
    "angular-cookies": "angular/bower-angular-cookies#v1.2.23",
    "angular-route": "angular/bower-angular-route"
  },
  "browser": {
    "angular-cookies": "./node_modules/angular-cookies/angular-cookies.js"
  },
  "dependencies": {
    "angular": "^1.2.23",
    "angular-ui-router": "^0.2.11",
    "angular-mocks": "~1.2.x",
    "angular-animate": "~1.2.x",
    "angular-resource": "~1.2.x",
    "angular-route": "~1.2.x",
    "jquery": "^2.1.1",
    "express": "^4.11.1",
    "body-parser": "^1.10.2",
    "socket.io": "*",
    "mongoose": "~3.8.0"
  },
  "devDependencies": {
    "babelify": "6.0.1",
    "browserify-shim": "^3.8.0",
    "grunt": "^0.4.5",
    "grunt-browserify": "^3.5.0",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-cssmin": "~0.12.2",
    "grunt-contrib-jshint": "^0.11.0",
    "grunt-contrib-uglify": "~0.5.0",
    "grunt-contrib-watch": "0.6.1",
    "grunt-contrib-sass": "^0.9.0",
    "grunt-babel": "^5.0.0",
    "matchdep": "~0.1.2",
    "napa": "^1.2.0",
    "shelljs": "^0.2.6",
    "docco": "*",
    "jasmine-core": "^2.2.0",
    "jasmine-expect": "^2.0.0-beta1",
    "karma": "^0.12.31",
    "karma-chrome-launcher": "^0.1.7",
    "karma-coverage": "^0.2.7",
    "karma-jasmine": "^0.3.5",
    "karma-ng-html2js-preprocessor": "^0.1.2",
    "grunt-docco": "~0.3.3",
    "mocha": "~2.2.5",
    "grunt-mocha-test": "~0.12.7",
    "grunt-istanbul": "~0.5.0",
    "grunt-env": "~0.4.1",
    "chai": "~3.0.0",
    "should": "*",
    "supertest": "~1.0.1",
    "socket.io-client": "~1.3.5"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "angular": {
      "depends": "jquery:jQuery",
      "exports": "angular"
    },
    "angular-cookies": {
      "depends": "angular",
      "exports": "angular.module('ngCookies').name"
    },
    "angular-ui-router": {
      "depends": "angular"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cebrakadabra/ES6_chat.git"
  },
  "author": "Christoph Eberl, Bernhard Zweim√ºller",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cebrakadabra/ES6_chat/issues"
  },
  "homepage": "https://github.com/cebrakadabra/ES6_chat"
}
